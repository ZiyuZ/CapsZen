{
  "title": "CapsZen",
  "rules": [
    {
      "description": "CapsZen",
      "manipulators": [
        {
          "description": "Core: CapsLock acts as a modifier, setting 'caps_lock_is_on'. (Single press is disabled)",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "caps_lock_is_on",
                "value": 1
              }
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "caps_lock_is_on",
                "value": 0
              }
            }
          ],
          "type": "basic"
        },
        {
          "description": "CapsLock + ` : Toggle actual Caps Lock state (大小写锁定切换)",
          "from": {
            "key_code": "grave_accent_and_tilde",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "caps_lock"
            }
          ],
          "conditions": [
            {
              "name": "caps_lock_is_on",
              "type": "variable_if",
              "value": 1
            }
          ],
          "type": "basic"
        },
        {
          "description": "CapsLock + Option + h : Select Left",
